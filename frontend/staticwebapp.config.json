{
  "routes": [
    {
      "route": "/signed-out.html",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/signed-out-full.html",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/styles.css",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/.auth/login/aad?post_login_redirect_uri=/"
    }
  }
}
